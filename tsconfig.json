{
    "compilerOptions": {
      "lib": ["es5", "es6"],
      "outDir": "./dist/",
      "sourceMap": true,
      "noImplicitAny": true,
      "module": "commonjs",
      "target": "es5",
      "downlevelIteration": true,
      "allowJs": true,
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "esModuleInterop": true,
      "baseUrl": ".",
      "paths": {
        "src/*": ["src/*"],
      }
    },
    "include": ["src"]
}
  